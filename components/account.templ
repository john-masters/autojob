package components

import "autojob/models"
import "fmt"

templ Account(user models.User) {
	@Layout("Account") {
		<main>
			<a href="/auth/logout">Logout</a>
			<h2>Welcome to the authorised zone</h2>
			<p>{ fmt.Sprint(user.ID) }</p>
			<p>{ user.FirstName }</p>
			<p>{ user.LastName }</p>
			<p>{ user.Email }</p>
			<p>{ user.Password }</p>
		</main>
	}
}
